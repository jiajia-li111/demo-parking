<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.plms.mapper.TVehicleMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.plms.model.entity.TVehicle">
        <id column="vehicle_id" property="vehicleId" />
        <result column="license_plate" property="licensePlate" />
        <result column="vehicle_type" property="vehicleType" />
        <result column="is_owner_car" property="isOwnerCar" />
        <result column="owner_id" property="ownerId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        vehicle_id, license_plate, vehicle_type, is_owner_car, owner_id
    </sql>

</mapper>
