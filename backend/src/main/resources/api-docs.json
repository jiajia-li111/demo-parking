  "components": {
    "schemas": {
      "SysUser": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "string",
            "description": "用户唯一标识"
          },
          "username": {
            "type": "string",
            "description": "登录用户名"
          },
          "password": {
            "type": "string",
            "description": "加密密码"
          },
          "roleId": {
            "type": "string",
            "description": "关联角色ID"
          },
          "department": {
            "type": "string",
            "description": "所属部门"
          },
          "status": {
            "type": "string",
            "description": "状态"
          }
        },
        "description": "存储操作人员信息"
      },
      "ResultBoolean": {
        "type": "object",
        "properties": {
          "code": {
            "type": "integer",
            "description": "状态码（200=成功，其他=失败）",
            "format": "int32",
            "example": 200
          },
          "msg": {
            "type": "string",
            "description": "响应消息（成功/失败描述）",
            "example": "操作成功"
          },
          "data": {
            "type": "boolean",
            "description": "响应数据（成功时返回，失败时为null）"
          },
          "success": {
            "type": "boolean"
          }
        },
        "description": "统一响应结果"
      },
      "MonthlyCard": {
        "type": "object",
        "properties": {
          "cardId": {
            "type": "string",
            "description": "月卡唯一标识"
          },
          "vehicleId": {
            "type": "string",
            "description": "绑定车辆ID"
          },
          "issuerId": {
            "type": "string",
            "description": "发卡人ID"
          },
          "startDate": {
            "type": "string",
            "description": "生效时间",
            "format": "date-time"
          },
          "endDate": {
            "type": "string",
            "description": "到期时间",
            "format": "date-time"
          },
          "status": {
            "type": "string",
            "description": "状态"
          }
        },
        "description": "月卡"
      },
      "Gate": {
        "type": "object",
        "properties": {
          "gateId": {
            "type": "string",
            "description": "通道唯一标识"
          },
          "gateType": {
            "type": "string",
            "description": "通道类型"
          },
          "entranceId": {
            "type": "string",
            "description": "所属出入口"
          },
          "status": {
            "type": "string",
            "description": "状态"
          }
        },
        "description": "停车场通道"
      },
      "ExitResultVO": {
        "type": "object",
        "properties": {
          "sessionId": {
            "type": "string",
            "description": "停车会话ID（关联本次停车记录）",
            "example": "s20241105001"
          },
          "parkingHours": {
            "type": "number",
            "description": "停车时长（单位：小时，保留1位小数）",
            "format": "double",
            "example": 3.5
          },
          "payAmount": {
            "type": "number",
            "description": "应付金额（单位：元）",
            "example": 11.0
          },
          "payMethod": {
            "type": "string",
            "description": "支付方式（01=微信，02=支付宝，03=现金）",
            "example": "01"
          },
          "payMethodName": {
            "type": "string",
            "description": "支付方式名称",
            "example": "微信支付"
          },
          "payStatus": {
            "type": "string",
            "description": "支付状态（01=成功，02=失败，03=退款）",
            "example": "01"
          },
          "payStatusDesc": {
            "type": "string",
            "description": "支付状态描述",
            "example": "支付成功"
          },
          "payTime": {
            "type": "string",
            "description": "支付时间（yyyy-MM-dd HH:mm:ss）",
            "example": "2024-11-05 12:30:00"
          },
          "exitTime": {
            "type": "string",
            "description": "出场时间（yyyy-MM-dd HH:mm:ss）",
            "example": "2024-11-05 12:30:00"
          },
          "pass": {
            "type": "boolean",
            "description": "是否放行（true=放行，false=拦截）",
            "example": true
          },
          "message": {
            "type": "string",
            "description": "提示信息",
            "example": "出场成功，感谢使用"
          }
        },
        "description": "车辆出场结果信息"
      },
      "ResultExitResultVO": {
        "type": "object",
        "properties": {
          "code": {
            "type": "integer",
            "description": "状态码（200=成功，其他=失败）",
            "format": "int32",
            "example": 200
          },
          "msg": {
            "type": "string",
            "description": "响应消息（成功/失败描述）",
            "example": "操作成功"
          },
          "data": {
            "$ref": "#/components/schemas/ExitResultVO"
          },
          "success": {
            "type": "boolean"
          }
        },
        "description": "统一响应结果"
      },
      "EntryResultVO": {
        "type": "object",
